  <xs:complexType name="EXTENSION-AllegedBullyingVictim">
    <xs:annotation>
      <xs:documentation>This represents the victims of alleged bullying incidents.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Identifies the student involved as the alleged bullying victim.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>This relates the school to where the alleged bullying incident occurred.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AllegedIncidentDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date of the alleged bullying incident.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BullyingCategory" type="EXTENSION-BullyingCategoryDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Indicates the type of bullying.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-BullyingCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="FollowupComment" type="EXTENSION-Comment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the follow up detail regarding the alleged bullying incident.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IncidentDescription" type="Description" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The description of the alleged bullying incident.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IncidentLocation" type="IncidentLocationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the location where alleged bullying incident occurred.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>IncidentLocationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="MultiplePriorEvent" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if student was involved in multiple prior alleged bullying incidents.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReasonIncidentRelatedTo" type="EXTENSION-IncidentRelatedToDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Indicates the reason for the alleged bullying incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IncidentRelatedToDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>Indicates the date on which the alleged bullying incident was reported.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReportedBy" type="EXTENSION-ReportedBy" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the person who reported the alleged bullying incident.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarDateExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CalendarDate">
        <xs:sequence>
          <xs:element name="Cycle" type="EXTENSION-CycleDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the code for specifying the cycle day.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CycleDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="MembershipDayValue" type="EXTENSION-MembershipDayValue" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the value of membership for the day.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CIPCourse">
    <xs:annotation>
      <xs:documentation>Identifies courses that are CIP.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="CIPCode" type="CIPCode">
            <xs:annotation>
              <xs:documentation>Indicates the code for the CIP course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Pathway" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the pathway.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FundingLevel" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the funding level.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Cluster" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the cluster.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseName" type="EXTENSION-CourseName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the name of the course.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ClassPeriodExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ClassPeriod">
        <xs:sequence>
          <xs:element name="AttendancePeriodOrder" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Number indicating the order that the period should be listed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Description" type="Description" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Description of the attendance period.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Cycle" type="EXTENSION-Cycle" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Indicates a cycle day.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Course">
        <xs:sequence>
          <xs:element name="Active" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The current status of the course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="APExamCode" type="EXTENSION-APExamCodeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the code for the AP exam.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-APExamCodeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="AveragePassTimeMinutesPerWeek" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the number of times allowed in the hall per week for the course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CoreSubject" type="EXTENSION-CoreSubjectDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the core subject associated with the course.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CoreSubjectDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="FractionOfYearOffered" type="EXTENSION-FractionOfYearOffered" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The fraction of a year for the Coop or DO student (i.e. 1.0 for full year, .50 for half year).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MinutesPerWeekAverage" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the minutes per week the course meets.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SecondaryStaffClassroomServiceStatus" type="EXTENSION-ServiceStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the status of the service of the secondary staff in the classroom.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ServiceStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StaffCredentialField" type="CredentialFieldDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the specialty requirement for the staff teaching the course.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>CredentialFieldDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SecondaryStaffCredentialField" type="CredentialFieldDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the specialty requirement for the secondary staff teaching the course.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>CredentialFieldDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SecondaryStaffRole" type="EXTENSION-StaffRoleDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the role of the secondary staff for the course.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StaffRoleDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="UnitCountExceptionSchoolReference" type="SchoolReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifes school with a unit count exception.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIncidentCode">
    <xs:annotation>
      <xs:documentation>This represents the additional attributes required for the Behavior descriptor.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="Behavior" type="BehaviorDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Identifies the type of behavior or discipline incident code.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>BehaviorDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Felony" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the incident behavior is a felony.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="VictimRequired" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if incident behavior requires a victim.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIncidentExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineIncident">
        <xs:sequence>
          <xs:element name="Referral" type="EXTENSION-ReferralDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Indicates the type of referral made for the incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ReferralDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="IncidentTimeFrame" type="EXTENSION-IncidentTimeFrameDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the code identifying the time frame for the incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IncidentTimeFrameDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="LawEnforcementBadgeNumber" type="EXTENSION-Number" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the badge number of the responding officer to the incident.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LawEnforcementDepartment" type="EXTENSION-LawEnforcementDepartment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the police department to which the incident was reported.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LawEnforcementNotifyDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date on which incident was reported to police.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LawEnforcementOfficerName" type="EXTENSION-LawEnforcementOfficerName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The name of police officer who responded to the complaint.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IncidentReason" type="EXTENSION-IncidentReason" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Indicates the category the incident relates to.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineIncidentLastModifiedDate" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date and time the table row was last changed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineIncidentChangeUID" type="EXTENSION-ChangeUID" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the person who last changed the table row.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineIncidentCreationUID" type="EXTENSION-CreationUID" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the person who created the table row.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIssue">
    <xs:annotation>
      <xs:documentation>This represents discipline issues entered through the teacher access center.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="IssueIdentifier" type="EXTENSION-IssueIdentifier">
            <xs:annotation>
              <xs:documentation>A unique number that identifies the issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>This relates to the school the issue occurred.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date the issue occurred.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StaffReference" type="StaffReferenceType">
            <xs:annotation>
              <xs:documentation>The identifier of the staff who reported the issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueIncidentLocation" type="IncidentLocationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the location of the incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>IncidentLocationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueCategory" type="EXTENSION-IssueCategoryDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the type that classifies the issue.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IssueCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueResolved" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the teacher has resolved the issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueSource" type="EXTENSION-IssueSourceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates whwere the issue occurred. Used for further validation of the Issue_source_detail.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IssueSourceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueSourceDetail" type="EXTENSION-Detail" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Provides detail of where the issue occurred.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueStatus" type="EXTENSION-IssueStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the status of the issue.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IssueStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Related" type="EXTENSION-Related" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The identifier of the related issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Comment" type="EXTENSION-Comment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the description of the issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseSession" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The course session that goes with the issue_source_detail (section_key) when issue source = 'C'.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIssueAction">
    <xs:annotation>
      <xs:documentation>This represents the action associated with a discipline issue.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="DisciplineIssueReference" type="EXTENSION-DisciplineIssueReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the discipline issue associated with the action.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EnteredSequence" type="EXTENSION-Sequence">
            <xs:annotation>
              <xs:documentation>Allows multiple corrective action to be inserted under the same date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActionEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date of an assigned action.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActionStartDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The start date of an assigned action.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActionCompleted" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the corrective action was actually completed by the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Comment" type="EXTENSION-Comment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>This represents the comments concerning the action.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IssueActionCategory" type="EXTENSION-IssueActionCategoryDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the code of the corrective action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IssueActionCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ParentsContacted" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if parent or guardian was contacted regarding the issue and the action.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIssueRefer">
    <xs:annotation>
      <xs:documentation>This represents the referral information on discipline issues that were referred.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="DisciplineIssueReference" type="EXTENSION-DisciplineIssueReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the DisciplineIssue associated with the DisciplineIssueRefer.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReferDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date the referral was sent.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReferSequence" type="EXTENSION-Sequence">
            <xs:annotation>
              <xs:documentation>Allows multiple referrals for the same issue on the same date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReferIssueStatus" type="EXTENSION-IssueStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the status of the referral.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-IssueStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Comment" type="EXTENSION-Comment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Provides the description of the referral.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineIncidentReference" type="DisciplineIncidentReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Reference to the DisciplineIncident associated with the DisciplineIssueRefer.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIssueStudent">
    <xs:annotation>
      <xs:documentation>This represents the student associated with the discipline issue.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="DisciplineIssueReference" type="EXTENSION-DisciplineIssueReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the DisciplineIssue associated with a student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>The identifier of the student involved in the discipline issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Comment" type="EXTENSION-Comment" minOccurs="0">
            <xs:annotation>
              <xs:documentation>provides description of the issue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AdminAssignedStudentParticipationCode" type="StudentParticipationCodeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates how the disciplinarian thinks the student was involved in the issue.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>StudentParticipationCodeDescriptor</ann:Descriptor>
